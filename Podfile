# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'LunchVote' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for LunchVote
  pod 'Bolts' 
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'
  pod 'SnapKit'
  
  target 'LunchVoteTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LunchVoteUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
